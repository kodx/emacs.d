function fact(i)
   if i == 0 then
      return 1
   else
      if i == 1 then
         return 1
      else
         if i == 2 then
            return 2
         else
            fact = 2
            for n=3,i do
               fact = fact * n
            end
            return fact
         end
      end
   end
end
